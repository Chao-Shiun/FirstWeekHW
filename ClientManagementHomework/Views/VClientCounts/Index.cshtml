@model IEnumerable<ClientManagementHomework.Models.VClientCount>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.客戶名稱)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.公司聯絡人數)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.公司銀行資訊數)
        </th>

        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.客戶名稱)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.公司聯絡人數)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.公司銀行資訊數)
            </td>
        </tr>
        }
    </table>